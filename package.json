{
	"name": "@jjuidev/cs",
	"version": "0.0.2",
	"description": "cs (claude switch) is helper CLI to switch claude code provider",
	"license": "MIT",
	"author": "jjuidev hi@jjuidev.com",
	"homepage": "https://github.com/jjuidev/cs.git#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/jjuidev/cs.git"
	},
	"bugs": {
		"url": "https://github.com/jjuidev/cs/issues"
	},
	"type": "module",
	"exports": {
		".": {
			"require": {
				"types": "./dist/types/index.d.ts",
				"default": "./dist/cjs/index.cjs"
			},
			"import": {
				"types": "./dist/types/index.d.ts",
				"default": "./dist/esm/index.js"
			}
		}
	},
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.js",
	"bin": {
		"cs": "./dist/cli/cs-cli.cjs"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"changeset": "changeset",
		"release": "npm run build && changeset publish",
		"unpublish": "npm unpublish @jjuidev/cs --force",
		"version": "changeset version"
	},
	"dependencies": {
		"@clack/prompts": "^1.0.0",
		"commander": "^14.0.2",
		"consola": "^3.4.2",
		"semver": "^7.7.3"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@types/node": "^25.1.0",
		"@types/semver": "^7.7.1",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3"
	},
	"keywords": [
		"cs",
		"claude,",
		"claude-code",
		"claude-code-cli",
		"claude-code-switch-cli",
		"claude-switch-provider",
		"claude-code-switch-provider"
	]
}
